{
  "name": "virtual-audio-graph",
  "version": "1.0.0",
  "description": "Library for declaratively manipulating the Web Audio API",
  "keywords": [
    "api",
    "audio",
    "audiograph",
    "declarative",
    "functional",
    "graph",
    "virtual-audio-graph",
    "virtual",
    "web"
  ],
  "main": "dist",
  "module": "esm",
  "repository": {
    "type": "git",
    "url": "https://github.com/benji6/virtual-audio-graph.git"
  },
  "scripts": {
    "build": "tsc && rollup -c",
    "docs": "tsc -p docsSrc && rollup -c docsSrc/rollup.config.js && rm -r docsSrc/esm",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "spec": "yarn build && tape 'test/**/*.js'",
    "spec:watch": "nodemon --watch src --watch test --exec yarn spec",
    "test": "yarn lint && tsc && eslint test && yarn spec",
    "test:watch": "nodemon --watch src --watch test --exec npm t"
  },
  "author": "Ben Hall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benji6/virtual-audio-graph/issues"
  },
  "homepage": "https://github.com/benji6/virtual-audio-graph",
  "devDependencies": {
    "@std/esm": "^0.23.4",
    "eslint": "^4.19.0",
    "eslint-config-benji6": "^1.3.1",
    "nodemon": "^1.17.2",
    "rollup": "^0.56.5",
    "tape": "^4.9.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "^2.7.2",
    "web-audio-test-api": "^0.5.2"
  },
  "@std": {
    "esm": {
      "esm": "js"
    }
  }
}
